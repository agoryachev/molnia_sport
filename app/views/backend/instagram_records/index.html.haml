= render 'backend/shared/blocks/subnav', model: 'instagram_records'
= render 'backend/shared/blocks/filter', model: :instagram_records
- if @instagram_records && @instagram_records.length > 0
  %table.table.table-condensed.table-striped
    %thead
      %tr
        %th #
        %th От кого
        %th Ссылка
        %th Дата публикации
        %th.centered
          %span.glyphicon.glyphicon-eye-open
        %th
    %tbody=render @instagram_records
  .help-block
    %small
      %span.label.label-default &nbsp;&nbsp;&nbsp;
      Не опубликовано
    = will_paginate(@instagram_records, renderer: BootstrapPagination::Rails)
- else
  = render 'backend/shared/no_results_message'