= render 'backend/shared/blocks/subnav', model: 'instagram_persons'
- if @instagram_persons && @instagram_persons.length > 0
  %table.table.table-condensed.table-striped
    %thead
      %tr
        %th #
        %th Имя
        %th Id
        %th Ссылка
        %th Дата публикации
    %tbody=render @instagram_persons
  .help-block
    = will_paginate(@instagram_persons, renderer: BootstrapPagination::Rails)
- else
  = render 'backend/shared/no_results_message'