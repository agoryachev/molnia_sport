=render 'subnav'

- if @groups && @groups.length > 0
  %table.table.table-condensed.table-striped
    %thead
      %tr
        %th.span1 #
        %th.centered Название группы
        %th.centered Количество сотрудников в группе
        %th.span1
    %tbody
      =render @groups
  = will_paginate(@groups, renderer: BootstrapPagination::Rails)
- else
  = render 'backend/shared/no_results_message'