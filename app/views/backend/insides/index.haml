= render 'backend/shared/blocks/subnav', model: 'insides'

- if @insides && @insides.length > 0
  %table.table.table-condensed.table-striped
    %thead
      %tr
        %th #
        %th Содержание
        %th.centered
          %span.glyphicon.glyphicon-eye-open
        %th.manage
    %tbody=render @insides
  .help-block
    %small
      %span.label.label-default &nbsp;&nbsp;&nbsp;
      Не опубликовано
    = will_paginate(@insides, renderer: BootstrapPagination::Rails)
- else
  = render 'backend/shared/no_results_message'
