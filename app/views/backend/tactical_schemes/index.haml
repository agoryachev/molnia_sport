= render 'backend/shared/blocks/subnav', model: 'tactical_schemes'
= render 'backend/shared/blocks/filter', model: :tactical_schemes
- if @tactical_schemes && @tactical_schemes.length > 0
  %table.table.table-condensed.table-striped
    %thead
      %tr
        %th #
        %th{width: '80%'} Описание
        %th.centered
          %span.glyphicon.glyphicon-eye-open
        %th
    %tbody=render @tactical_schemes
  .help-block
    %small
      %span.label.label-default &nbsp;&nbsp;&nbsp;
      Не опубликовано
  = will_paginate(@tactical_schemes, renderer: BootstrapPagination::Rails)
- else
  = render 'backend/shared/no_results_message'