= render 'backend/shared/blocks/subnav', model: 'countries'
- if @countries && @countries.length > 0
  %table.table.table-condensed.table-striped
    %thead
      %tr
        %th #
        %th Заголовок
        %th Категории
        %th
    %tbody=render @countries
  .help-block
    %small
      %span.label.label-default &nbsp;&nbsp;&nbsp; 
      Не опубликовано  
  = will_paginate(@countries, renderer: BootstrapPagination::Rails)
- else
  = render 'backend/shared/no_results_message'