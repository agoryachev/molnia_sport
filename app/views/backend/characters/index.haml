= render 'backend/shared/blocks/subnav', model: 'characters'
- if @characters && @characters.length > 0
  %table.table.table-condensed.table-striped
    %thead
      %tr
        %th #
        %th Название
        %th
    %tbody=render @characters
  = will_paginate(@characters, renderer: BootstrapPagination::Rails)
- else
  = render 'backend/shared/no_results_message'