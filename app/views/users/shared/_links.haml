- if controller_name != 'sessions'
  = link_to 'Регистрация', new_session_path(resource_name)
  %br/
- if devise_mapping.registerable? && controller_name != 'registrations'
  = link_to 'Войти', new_registration_path(resource_name)
  %br/
- if devise_mapping.confirmable? && controller_name != 'confirmations'
  = link_to "Не получил письмо с подтверждением", new_confirmation_path(resource_name)
  %br/
- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks'
  = link_to "Не получил письмо с инструкциями по восстановлению", new_unlock_path(resource_name)
  %br/
- if devise_mapping.omniauthable?
  - resource_class.omniauth_providers.each do |provider|
    = link_to "Войти с #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider)
    %br/